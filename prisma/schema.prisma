generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model Scales {
  ID                   Int     @id(map: "PK__Scales__3214EC276993C39A") @default(autoincrement())
  name                 String  @db.NVarChar(100)
  tonic                String  @db.NVarChar(10)
  notesInScale         String  @db.NVarChar(200)
  stepFormula          String? @db.NVarChar(200)
  scaleIntervalFormula String? @db.NVarChar(300)
  chordalHarmonization String? @db.NVarChar(500)
}
